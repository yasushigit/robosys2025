import sys

def main():
    text = sys.stdin.read()

    lines = text.count("\n")
    if text and not text.endswith("\n"):
        lines += 1

    words = len(text.split())
    chars = len(text)

    print(f"lines:{lines} words:{words} chars:{chars}")

if __name__ == "__main__":
    main()