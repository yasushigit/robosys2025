import sys

def tonum(s):
      try:
          return int(s)
      except:
          return float(s)

lines = list(sys.stdin)

ans_plus_1 = 0.0
ans_square_1 = 0.0
ans_plus_2 = 0.0
ans_square_2 = 0.0

# 計算1: Σk および Σk^2
for line in lines:
    line = line.rstrip()
    num = tonum(line)
    ans_plus_1 += num
    ans_square_1 += num ** 2

# 計算2: Σk および Σk^2
first_line = tonum(lines[0].strip())
last_line = tonum(lines[-1].strip())
ans_plus_2 = ((last_line * (last_line + 1)) / 2) - (((first_line-1) * ((first_line - 1) + 1)) / 2)
ans_square_2 = ((last_line * (last_line + 1) * (2 * last_line + 1)) / 6) - (((first_line -1) * ((first_line - 1) + 1) * (2 * (first_line-1) + 1)) / 6)

# 結果の表示
print(ans_plus_1)
print(ans_square_1)
print(ans_plus_2)
print(ans_square_2)