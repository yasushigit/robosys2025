import sys

def main():
    text = sys.stdin.read()

    lines = text.splitlines()
    num_lines = len(lines)

    num_words = len(text.split())

    num_chars = len(text.rstrip("\n"))

    print(f"lines:{num_lines} words:{num_words} chars:{num_chars}")

if __name__ == "__main__":
    main()